<div class="social">
    {% for socials in social.all %}
    <a href="{{ socials.url }}" class="social__items" target="_blank" rel="noopener noreferrer">
      <img src="{{ socials.icon }}" alt="{{ socials.name }}" title="{{ socials.name }}" height="50px" width="50px">
    </a>
    {% endfor %}
</div>
